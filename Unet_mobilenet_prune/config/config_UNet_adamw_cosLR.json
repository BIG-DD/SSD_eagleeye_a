{
    "name": "parking_slot",
    "n_gpu": 1,
    
    "arch": {
        "type": "UNet",
        "args": {
            "backbone": "mobilenetv2",
            "num_classes": 2,
            "pretrained_backbone": null
        }
    },

	"train_loader": {
		"type": "SegmentationDataLoader",
		"args":{
			"images_folder": "C:\\DataSet\\line_jpg_png\\images\\train\\",
			"seg_annotations_folder": "C:\\DataSet\\line_jpg_png\\ll_seg_annotations\\train\\",
			"color_channel": "RGB",
			"resize": 256,
			"padding_value": 0,
			"is_training": true,
			"noise_std": 3,
			"crop_range": [0.90, 1.0],
			"flip_hor": 0.5,
			"rotate": 0.0,
			"angle": 10,
			"normalize": false,
			"one_hot": false,
			"shuffle": true,
			"batch_size": 32,
			"n_workers": 0,
			"pin_memory": true
		}
	},

	"valid_loader": {
		"type": "SegmentationDataLoader",
		"args":{
			"images_folder": "C:\\DataSet\\line_jpg_png\\images\\val\\",
			"seg_annotations_folder": "C:\\DataSet\\line_jpg_png\\ll_seg_annotations\\val\\",
			"color_channel": "RGB",
			"resize": 256,
			"padding_value": 0,
			"is_training": false,
			"normalize": false,
			"one_hot": false,
			"shuffle": false,
			"batch_size": 32,
			"n_workers": 0,
			"pin_memory": true
		}
	},

	"optimizer": {
		"type": "AdamW",
		"args":{
			"lr": 1e-3,
			"betas": (0.9, 0.999),
			"eps": 1e-6,
			"weight_decay": 1e-8,
		}
	},

	"loss": "dice_loss",
	"metrics": [
		"miou"
	],

	"lr_scheduler": {
		"type":"CosineAnnealingWarmRestarts",
		"args":{
			"T_0": 5,
			"T_mult": 2
		}
	},

	"trainer": {
		"epochs": 260,
		"save_dir": "D:\\project\\python\\park_line_three\\data\\unet_runs\\6_11\\",
		"save_freq": null,
		"verbosity": 2,
		"monitor": "valid_loss",
		"monitor_mode": "min"
	},

	"visualization":{
		"tensorboardX": true,
		"log_dir": "D:\\project\\python\\park_line_three\\data\\unet_runs\\6_11\\"
	}
}